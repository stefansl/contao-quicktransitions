{
    "name": "stefansl/contao-quicktransitions",
    "type": "contao-bundle",
    "description": "Fancy transitions for content elements in Contao",
    "homepage":"https://clickpress.de",
    "license": "LGPL-3.0-or-later",
    "authors":[
        {
            "name":"CLICKPRESS",
            "homepage":"https://clickpress.de",
            "role":"Developer"
        }
    ],
    "require":{
        "contao/core-bundle": "^^5.3"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "extra": {
        "contao-manager-plugin": "Clickpress\\ContaoQuicktransitions\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Clickpress\\ContaoQuicktransitions\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/stefansl/contao-quicktransitions/issues",
        "source": "https://github.com/stefansl/contao-quicktransitions"
    }
}
